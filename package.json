{
  "name": "svgmaker-sdk",
  "version": "0.1.0",
  "description": "Official Node.js SDK for SVGMaker API",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "files": [
    "dist/",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm && npm run build:types",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:types": "tsc -p tsconfig.types.json",
    "clean": "rimraf dist",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "manual-test": "npx ts-node --project tsconfig.cjs.json manual-test.ts",
    "prepublishOnly": "npm run test && npm run lint"
  },
  "keywords": [
    "svg",
    "svgmaker",
    "api",
    "sdk",
    "vector",
    "graphics",
    "typescript"
  ],
  "author": "SVGMaker",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/GenWaveLLC/svgmaker-sdk-node.git"
  },
  "bugs": {
    "url": "https://github.com/GenWaveLLC/svgmaker-sdk-node/issues"
  },
  "homepage": "https://github.com/GenWaveLLC/svgmaker-sdk-node#readme",
  "engines": {
    "node": ">=16.0.0"
  },
  "devDependencies": {
    "@types/form-data": "^2.2.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^18.19.110",
    "@types/node-fetch": "^2.6.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.7.0",
    "msw": "^1.2.1",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.10",
    "ts-jest": "^29.3.4",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "abort-controller": "^3.0.0",
    "form-data": "^4.0.2",
    "node-fetch": "^2.7.0",
    "p-retry": "^5.1.2",
    "p-throttle": "^5.1.0",
    "zod": "^3.25.42"
  }
}
