# SVGMaker SDK Environment Variables
# Copy this file to .env and update with your actual values

# Your SVGMaker API Key (required)
SVGMAKER_API_KEY=your-api-key-here

# Optional: Custom API base URL (default: https://svgmaker.io/api)
# SVGMAKER_BASE_URL=https://svgmaker.io/api

# Optional: Request timeout in milliseconds (default: 30000)
# SVGMAKER_TIMEOUT=60000

# Optional: Enable debug logging (default: false)
# SVGMAKER_DEBUG=true
